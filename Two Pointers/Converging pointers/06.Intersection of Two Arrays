class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        sort(nums1.begin(),nums1.end());
        sort(nums2.begin(),nums2.end());
        int m=nums1.size();
        int n=nums2.size();
        int i=0;
        int j=0;
        unordered_set<int>s;
        while(i<m && j<n)
        {
            if(nums1[i]<nums2[j]){
                i++;
            }
            else if(nums2[j]<nums1[i])
            {
                j++;
            }
            else
            {

                    s.insert(nums1[i]);
                     i++;
                     j++;
                
               
            }
        }
        return vector<int>(s.begin(),s.end());
    }
};
